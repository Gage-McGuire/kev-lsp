{
    "name": "kev-lsp",
    "displayName": "KEV LSP",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:kev"
    ],
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "kev",
                "extensions": [
                    ".kev"
                ],
                "aliases": [
                    "KEV"
                ],
                "icon": {
                    "light": "./icons/kev-icon.svg",
                    "dark": "./icons/kev-icon.svg"
                }
            }
        ],
        "iconThemes": [
            {
                "id": "kev-icons",
                "label": "KEV Icons",
                "path": "./kev-icon-theme.json"
            }
        ]
    },
    "dependencies": {
        "dotenv": "^17.2.3",
        "vscode-languageclient": "^9.0.1"
    }
}
